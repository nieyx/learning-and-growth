extends ../layout

block content
	.container
		.row
			.col-md-7 
				embed(src="#{moive.flash}", allowFullScreen="true", quality="high", width="710", height="600", align="middle", type="application/x-shockwave-flash")
			.col-md-5
				dl.dl-horizontal
					dt 电影名字
					dd= moive.title
					dt 导演
					dd= moive.doctor
					dt 国家
					dd= moive.country
					dt 语言
					dd= moive.language
					dt 上映名字
					dd= moive.year
					dt 简介
					dd= moive.summary